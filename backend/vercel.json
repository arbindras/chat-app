{
    "version":2,
    "builds":[
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"           
        }
    ],
    "headers": [
    {
      "for": "/api/*", 
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*" 
        }
      ]
    }
  ]
}
